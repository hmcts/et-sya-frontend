{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "_comment": "This config was generated using 'stryker init'. Please take a look at: https://stryker-mutator.io/docs/stryker-js/configuration/ for more information.",
  "packageManager": "yarn",
  "reporters": ["html", "clear-text", "progress"],

  "testRunner": "jest",
  "testRunner_comment": "Take a look at https://stryker-mutator.io/docs/stryker-js/jest-runner for information about the jest plugin.",
  "coverageAnalysis": "off",
  "maxTestRunnerReuse": 100,
  "mutate": [
    "src/main/helper/**/*.ts",
    "src/main/components/form/**/*.ts",
    "src/main/services/CaseService.ts",
    "src/main/services/AxiosErrorAdapter.ts",
    "src/main/services/CacheService.ts",
    "src/main/auth/**/*.ts",
    "src/main/utils/RedisUtils.ts",
    "src/main/controllers/helpers/ApplicationStateHelper.ts",
    "src/main/controllers/helpers/CaseHelpers.ts",
    "src/main/controllers/helpers/CaseDocumentHelper.ts",
    "src/main/controllers/helpers/DocumentHelpers.ts",
    "src/main/controllers/helpers/RedirectHelpers.ts",
    "src/main/controllers/helpers/RouterHelpers.ts",
    "src/main/controllers/helpers/CitizenHub*.ts",
    "src/main/controllers/helpers/ContactTheTribunal*.ts",
    "src/main/controllers/helpers/Respondent*Helper.ts",
    "src/main/controllers/helpers/RespondToApplicationHelper.ts",
    "src/main/controllers/helpers/Hearing*Helper.ts",
    "src/main/controllers/helpers/Bundles*Helper.ts",
    "src/main/decorators/FormSubmissionCheck.ts",
    "!**/__mocks__/**",
    "!**/mocks/**",
    "!**/*.d.ts"
  ],
  "jest": {
    "projectType": "custom",
    "configFile": "jest.unit.config.js",
    "enableFindRelatedTests": true
  }
}