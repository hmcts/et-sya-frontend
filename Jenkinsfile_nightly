#!groovy

properties([
// H allow predefined but random minute see https://en.wikipedia.org/wiki/Cron#Non-standard_characters
pipelineTriggers([cron('H 05 * * *')])
parameters([
        string(name: 'TEST_URL', defaultValue: 'https://et-sya-ui.aat.platform.hmcts.net', description: 'The URL you want to run tests against'),
    ])
])

@Library("Infrastructure")

def type = "nodejs"
def product = "et"
def component = "sya"

withNightlyPipeline(type, product, component) {

}
